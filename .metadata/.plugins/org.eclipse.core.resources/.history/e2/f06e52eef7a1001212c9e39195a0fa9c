public class Rectangle
{
	private int length;
	private int width;

	public Rectangle(int len, int w)
	{
		length = len;
		width = w;
	}

	public int getLength()
	{
		return length;
	}

	public int getWidth()
	{
		return width;
	}

	public String toString()
	{
		return length + "x" + width;
	}
}