
import java.io.*;

/**
 * 
 * @author Dennis Moon
 * @version Feb 7, 2013
 * 
 * Revision:
 *     Feb 7, 2013
 */
public class Test
{
    public static void main(String[] args) throws IOException
    {
        problem1();
    }
    
    public static void problem1() throws IOException
    {
        BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
        int N = Integer.parseInt(br.readLine());
        
        int count = 2;
        for (int i=2; i*i<=N; i++)
            if (N%i==0)
                count++;
        
        System.out.println(count);
    }
    
    public static void problem2() throws IOException
    {
        BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
        int last = Integer.parseInt(br.readLine())-1;
        String str = br.readLine();
        int[] N = new int[last+1];
        int index = 0;
        for (int i=0; i<str.length(); i+=2)
        {
            N[index] = Integer.parseInt(str.substring(i, i+1));
            index++;
        }
        
        int current = 0;
        int score = 0;
        int min = last+1;
        boolean done = false;
        
        while(!done)
        {
            if (N[current]==1)
                current++;
            else if (N[current]+current > last)
            {
                score++;
                current++;
            }
            else if (N[current]+current <= last)
                current += N[current];
            
            if (current==last)
            {
                done = true;
                if (score<min)
                    min = score;
            }
        }
        
        System.out.println(min);
    }
}
